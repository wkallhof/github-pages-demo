<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title }}</title>
    <link rel="stylesheet" href="{{ '/style.css' | url }}"/>
  </head>
  <body>
    <nav>
      <h2>Backend / API</h2>
      <ul>
      {%- for page in collections.api -%}
        <li><a href="{{ page.url | url }}">{{ page.data.title }}</a></li>
      {%- endfor -%}
      </ul>

      <h2>Web Platform</h2>
      <ul>
      {%- for page in collections.web -%}
        <li><a href="{{ page.url | url }}">{{ page.data.title }}</a></li>
      {%- endfor -%}
      </ul>
    </nav>
    {{ content | safe }}
  </body>
</html>